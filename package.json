{
  "name": "bounipun-ecom",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "frontend:generate": "nuxt generate",
    "dev:nuxt": "NUXT_HOST=0.0.0.0 NUXT_PORT=8080 nuxt --spa",
    "frontend:build": "nuxt build --spa",
    "dev:server": "NODE_ENV=development nodemon --watch server --exec ts-eager server/index.ts",
    "server:build": "NODE_ENV=production node serverBundle.js",
    "buildx": "yarn run frontend:build && yarn run server:build",
    "build": "yarn run server:build",
    "start": "NODE_ENV=production node dist/server.js",
    "start:dev": "yarn run frontend:dev && yarn run server:dev",
    "up": "yarn run build && yarn run start",
    "stripe:listen": "stripe listen --forward-to localhost:3000/stripeWebhook" ,
    "backup-prod": "./db-transport backup bounipun-ecom-prod",
    "staging-db-restore": "./db-transport restore bounipun-ecom-stage"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@sendgrid/mail": "^7.4.7",
