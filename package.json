{
  "name": "bounipun-ecom",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "frontend:generate": "nuxt generate",
    "dev:nuxt": "NUXT_HOST=0.0.0.0 NUXT_PORT=8080 nuxt",
    "frontend:build": "nuxt build --spa",
    "dev:server": "NODE_ENV=development nodemon --watch server --exec ts-eager server/index.ts",
    "server:build": "NODE_ENV=production node serverBundle.js",
    "buildx": "yarn run frontend:build && yarn run server:build",
    "build": "yarn run server:build",
    "start": "NODE_ENV=production node test/server.js",
    "start:dev": "yarn run frontend:dev && yarn run server:dev",
    "up": "yarn run build && yarn run start"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "aws-sdk": "^2.863.0",
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gsap": "^3.6.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nuxt": "^2.14.12",
    "sharp": "^0.27.2",
    "slugify": "^1.4.7",
    "uuid": "^8.3.2",
    "v-calendar": "^2.3.0",
    "vue-ellipse-progress": "^1.3.0",
    "vue-inner-image-zoom": "^1.0.1",
    "vue-js-toggle-button": "^1.3.3",
    "vue-rocker-switch": "^1.2.4",
    "vue-switches": "^2.0.1",
    "vue2-hammer": "^2.1.2",
    "vuejs-auto-complete": "^0.9.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-resolve": "^0.0.14",
    "@nuxtjs/style-resources": "^1.0.0",
    "@types/express": "^4.17.11",
    "esbuild": "^0.9.2",
    "esbuild-node-externals": "^1.0.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.1",
    "ts-eager": "^1.1.3",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
